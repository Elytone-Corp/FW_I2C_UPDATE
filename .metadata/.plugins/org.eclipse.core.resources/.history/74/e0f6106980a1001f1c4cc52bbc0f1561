static bootloader_state_t current_state = BOOTLOADER_IDLE;
static bootloader_error_t last_error = BOOTLOADER_NO_ERROR;

static void SetError(bootloader_error_t error) {
	last_error = error;
	current_state = BOOTLOADER_ERROR;

}

